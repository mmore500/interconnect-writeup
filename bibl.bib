@article{ackley2018alife,
author = {Ackley, David H.},
title = {Digital protocells with dynamic size, position, and topology},
journal = {The 2018 Conference on Artificial Life: A Hybrid of the European Conference on Artificial Life (ECAL) and the International Conference on the Synthesis and Simulation of Living Systems (ALIFE)},
volume = {},
number = {},
